      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
  </head>
  <body>
    <nav class="navbar navbar-expand-md">
      <div class="container-fluid">
        <a href="" class="navbar-brand">
          <img src="/image/logo.png" class="weblogo" />
        </a>
        <!--mobile view-->
        <button
          class="navbar-toggler"
          data-bs-target="#a"
          data-bs-toggle="collapse"
          y
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <!--mobile view end-->

        <!--menu-->
        <div class="navbar-collapse collapse" id="a">
          <ul class="navbar-nav ms-auto">
            <li><a href="/dashboard" class="nav-link">Home</a></li>
            <li><a href="/aboutus" class="nav-link">About</a></li>
            <li><a href="/contact" class="nav-link">Contact</a></li>
            <div class="dropdown">
              <button
                class="nav-link drop"
                type="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
              >
                <%= n %>
              </button>
              <ul class="dropdown-menu">
                <li><a class="nav-link" href="/profile">Edit Profile</a></li>
                <li><a class="nav-link" href="/logout">Logout</a></li>
              </ul>
            </div>
          </ul>
          <img
            src="<%= image.url%>"
            width="100px"
            height="100px"
            style="border-radius: 50px"
          />
        </div>
        <!--menu end-->
      </div>
      <!--container-fluid end-->
    </nav>
    <!--nav end-->
  </body>
</html>
